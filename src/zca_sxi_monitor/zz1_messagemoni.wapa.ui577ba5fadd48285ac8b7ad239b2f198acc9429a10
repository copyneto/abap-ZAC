sap.ui.define(                                                                                                                                                                                                                                                 
  ["sap/ui/model/Filter", "sap/ui/comp/smartfilterbar/SmartFilterBar"],                                                                                                                                                                                        
  function (Filter, SmartFilterBar) {                                                                                                                                                                                                                          
    "use strict";                                                                                                                                                                                                                                              
    return {                                                                                                                                                                                                                                                   
      // assinatura obsoleta - utilizado na vers√£o SWD                                                                                                                                                                                                         
      // sap.ui.controller(                                                                                                                                                                                                                                    
      //   "br.com.trescoracoes.messagemonitor.ext.controller.ListReportExt",                                                                                                                                                                                  
      //   {                                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
      onInit: function () {                                                                                                                                                                                                                                    
        this.byId("ExecutionDe").setDateValue(new Date(Date.now() - 3600000));                                                                                                                                                                                 
        this.byId("ExecutionAte").setDateValue(                                                                                                                                                                                                                
          new Date(new Date().setHours(23, 59, 59, 999) + 1)                                                                                                                                                                                                   
        );                                                                                                                                                                                                                                                     
      },                                                                                                                                                                                                                                                       
      onBeforeRebindTableExtension: function (oEvent) {                                                                                                                                                                                                        
        var oBindingParams = oEvent.getParameter("bindingParams");                                                                                                                                                                                             
        oBindingParams.parameters = oBindingParams.parameters || {};                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
        var vDataDe = this.byId("ExecutionDe").getDateValue();                                                                                                                                                                                                 
        var vDataAte = this.byId("ExecutionAte").getDateValue();                                                                                                                                                                                               
        if (vDataAte == null)                                                                                                                                                                                                                                  
          oBindingParams.filters.push(                                                                                                                                                                                                                         
            new sap.ui.model.Filter("ExecutionDate", "BT", vDataDe, Date.now())                                                                                                                                                                                
          );                                                                                                                                                                                                                                                   
        else                                                                                                                                                                                                                                                   
          oBindingParams.filters.push(                                                                                                                                                                                                                         
            new sap.ui.model.Filter("ExecutionDate", "BT", vDataDe, vDataAte)                                                                                                                                                                                  
          );                                                                                                                                                                                                                                                   
      },                                                                                                                                                                                                                                                       
    };                                                                                                                                                                                                                                                         
  }                                                                                                                                                                                                                                                            
);                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               