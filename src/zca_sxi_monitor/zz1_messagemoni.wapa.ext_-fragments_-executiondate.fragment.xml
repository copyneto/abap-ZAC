<core:FragmentDefinition xmlns="sap.m" xmlns:smartfilterbar="sap.ui.comp.smartfilterbar" xmlns:core="sap.ui.core">                                                                                                                                             
<smartfilterbar:ControlConfiguration groupId="_BASIC"&nbsp;                                                                                                                                                                                                    
    key="dataHoraInicio"                                                                                                                                                                                                                                       
    label="Data Hora Inicio"                                                                                                                                                                                                                                   
    mandatory="mandatory"                                                                                                                                                                                                                                      
    visibleInAdvancedArea="true">                                                                                                                                                                                                                              
    <smartfilterbar:customControl>                                                                                                                                                                                                                             
          <DateTimePicker                                                                                                                                                                                                                                      
			id="ExecutionDe"                                                                                                                                                                                                                                            
			placeholder="Data/Hora Inicio"                                                                                                                                                                                                                              
			change="handleChange"                                                                                                                                                                                                                                       
			class="sapUiSmallMarginBottom"/>                                                                                                                                                                                                                            
    </smartfilterbar:customControl>                                                                                                                                                                                                                            
</smartfilterbar:ControlConfiguration>                                                                                                                                                                                                                         
<smartfilterbar:ControlConfiguration groupId="_BASIC"&nbsp;                                                                                                                                                                                                    
    key="dataHoraFim"                                                                                                                                                                                                                                          
    label="Data Hora Fim"                                                                                                                                                                                                                                      
    visibleInAdvancedArea="true">                                                                                                                                                                                                                              
    <smartfilterbar:customControl>                                                                                                                                                                                                                             
          <DateTimePicker                                                                                                                                                                                                                                      
			id="ExecutionAte"                                                                                                                                                                                                                                           
			placeholder="Data/Hora Fim"                                                                                                                                                                                                                                 
			change="handleChange"                                                                                                                                                                                                                                       
			class="sapUiSmallMarginBottom"/>                                                                                                                                                                                                                            
    </smartfilterbar:customControl>                                                                                                                                                                                                                            
</smartfilterbar:ControlConfiguration>                                                                                                                                                                                                                         
</core:FragmentDefinition>                                                                                                                                                                                                                                     